  "version": "0.2.0",
  "configurations": [
    {
      "name": "Web - Launch via NPM",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceRoot}",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run", "debug"
      ],
      "outputCapture": "std",
      "envFile": "${workspaceFolder}/.env",
      "port": 9229,
      "skipFiles": [
        "<node_internals>/**"
      ],
      // "program": "${workspaceFolder}/dist/index.js",
      // "preLaunchTask": "tsc: build - tsconfig.json",
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ]
    },

